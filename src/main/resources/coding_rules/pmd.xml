<?xml version="1.0"?>

<ruleset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         name="Custom Rules"
         xmlns="http://pmd.sourceforge.net/ruleset/2.0.0"
         xsi:schemaLocation="http://pmd.sourceforge.net/ruleset/2.0.0 https://pmd.sourceforge.io/ruleset_2_0_0.xsd">

    <description>
        Confidence rule sets
    </description>

    <!--#########################################################################-->
    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java.html#best-practices-->
    <!--Best Practices-->

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#abstractclasswithoutabstractmethod-->
    <rule ref="category/java/bestpractices.xml/AbstractClassWithoutAbstractMethod"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#arrayisstoreddirectly-->
    <rule ref="category/java/bestpractices.xml/ArrayIsStoredDirectly"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#avoidprintstacktrace-->
    <rule ref="category/java/bestpractices.xml/AvoidPrintStackTrace"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#avoidreassigningparameters-->
    <rule ref="category/java/bestpractices.xml/AvoidReassigningParameters"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#avoidstringbufferfield-->
    <rule ref="category/java/bestpractices.xml/AvoidStringBufferField"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#constantsininterface-->
    <rule ref="category/java/bestpractices.xml/ConstantsInInterface">
        <properties>
            <property name="ignoreIfHasMethods">
                <value>true</value>
            </property>
        </properties>
    </rule>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#defaultlabelnotlastinswitchstmt-->
    <rule ref="category/java/bestpractices.xml/DefaultLabelNotLastInSwitchStmt"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#forloopcanbeforeach-->
    <rule ref="category/java/bestpractices.xml/ForLoopCanBeForeach"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#loosecoupling-->
    <rule ref="category/java/bestpractices.xml/LooseCoupling"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#methodreturnsinternalarray-->
    <rule ref="category/java/bestpractices.xml/MethodReturnsInternalArray"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#missingoverride-->
    <rule ref="category/java/bestpractices.xml/MissingOverride"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#preservestacktrace-->
    <rule ref="category/java/bestpractices.xml/PreserveStackTrace"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#replacehashtablewithmap-->
    <rule ref="category/java/bestpractices.xml/ReplaceHashtableWithMap"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#replacevectorwithlist-->
    <rule ref="category/java/bestpractices.xml/ReplaceVectorWithList"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#systemprintln-->
    <rule ref="category/java/bestpractices.xml/SystemPrintln"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#unusedformalparameter-->
    <rule ref="category/java/bestpractices.xml/UnusedFormalParameter">
        <properties>
            <property name="checkAll">
                <value>true</value>
            </property>
        </properties>
    </rule>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#unusedlocalvariable-->
    <rule ref="category/java/bestpractices.xml/UnusedLocalVariable"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#unusedprivatefield-->
    <rule ref="category/java/bestpractices.xml/UnusedPrivateField"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#unusedprivatemethod-->
    <rule ref="category/java/bestpractices.xml/UnusedPrivateMethod"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#unusedprivatemethod-->
    <rule ref="category/java/bestpractices.xml/UseAssertEqualsInsteadOfAssertTrue"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#useassertnullinsteadofasserttrue-->
    <rule ref="category/java/bestpractices.xml/UseAssertNullInsteadOfAssertTrue"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#useassertsameinsteadofasserttrue-->
    <rule ref="category/java/bestpractices.xml/UseAssertSameInsteadOfAssertTrue"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#useasserttrueinsteadofassertequals-->
    <rule ref="category/java/bestpractices.xml/UseAssertTrueInsteadOfAssertEquals"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#usecollectionisempty-->
    <rule ref="category/java/bestpractices.xml/UseCollectionIsEmpty"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_bestpractices.html#usevarargs-->
    <rule ref="category/java/bestpractices.xml/UseVarargs"/>

    <!--######################################################################################-->

    <!--######################################################################################-->
    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java.html#code-style-->
    <!--Code Style-->

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#avoiddollarsigns-->
    <rule ref="category/java/codestyle.xml/AvoidDollarSigns"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#avoidprotectedfieldinfinalclass-->
    <rule ref="category/java/codestyle.xml/AvoidProtectedFieldInFinalClass"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#avoidprotectedmethodinfinalclassnotextending-->
    <rule ref="category/java/codestyle.xml/AvoidProtectedMethodInFinalClassNotExtending"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#avoidusingnativecode-->
    <rule ref="category/java/codestyle.xml/AvoidUsingNativeCode"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#booleangetmethodname-->
    <rule ref="category/java/codestyle.xml/BooleanGetMethodName"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#classnamingconventions-->
    <rule ref="category/java/codestyle.xml/ClassNamingConventions">
        <properties>
            <property name="interfacePattern">
                <value>I[A-Z][a-zA-Z0-9]*</value>
            </property>
            <property name="enumPattern">
                <value>E[A-Z][a-zA-Z0-9]*</value>
            </property>
        </properties>
    </rule>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#controlstatementbraces-->
    <rule ref="category/java/codestyle.xml/ControlStatementBraces"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#dontimportjavalang-->
    <rule ref="category/java/codestyle.xml/DontImportJavaLang"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#duplicateimports-->
    <rule ref="category/java/codestyle.xml/DuplicateImports"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#emptymethodinabstractclassshouldbeabstract-->
    <rule ref="category/java/codestyle.xml/EmptyMethodInAbstractClassShouldBeAbstract"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#extendsobject-->
    <rule ref="category/java/codestyle.xml/ExtendsObject"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#fielddeclarationsshouldbeatstartofclass-->
    <rule ref="category/java/codestyle.xml/FieldDeclarationsShouldBeAtStartOfClass"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#forloopshouldbewhileloop-->
    <rule ref="category/java/codestyle.xml/ForLoopShouldBeWhileLoop"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#genericsnaming-->
    <rule ref="category/java/codestyle.xml/GenericsNaming"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#identicalcatchbranches-->
    <rule ref="category/java/codestyle.xml/IdenticalCatchBranches"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#linguisticnaming-->
    <rule ref="category/java/codestyle.xml/LocalHomeNamingConvention"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#longvariable-->
    <rule ref="category/java/codestyle.xml/LongVariable">
        <properties>
            <property name="minimum">
                <value>40</value>
            </property>
        </properties>
    </rule>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#methodargumentcouldbefinal-->
    <rule ref="category/java/codestyle.xml/MethodArgumentCouldBeFinal"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#nopackage-->
    <rule ref="category/java/codestyle.xml/NoPackage"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#shortclassname-->
    <rule ref="category/java/codestyle.xml/ShortClassName">
        <properties>
            <property name="minimum">
                <value>4</value>
            </property>
        </properties>
    </rule>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#shortmethodname-->
    <rule ref="category/java/codestyle.xml/ShortMethodName"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#shortvariable-->
    <rule ref="category/java/codestyle.xml/ShortVariable">
        <properties>
            <property name="minimum">
                <value>2</value>
            </property>
        </properties>
    </rule>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#unnecessaryannotationvalueelement-->
    <rule ref="category/java/codestyle.xml/UnnecessaryAnnotationValueElement"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#unnecessaryconstructor-->
    <rule ref="category/java/codestyle.xml/UnnecessaryConstructor"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#unnecessaryfullyqualifiedname-->
    <rule ref="category/java/codestyle.xml/UnnecessaryFullyQualifiedName"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#unnecessarymodifier-->
    <rule ref="category/java/codestyle.xml/UnnecessaryModifier"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#unnecessaryreturn-->
    <rule ref="category/java/codestyle.xml/UnnecessaryReturn"/>

    <!--https://pmd.github.io/pmd-6.9.0/pmd_rules_java_codestyle.html#uselessqualifiedthis-->
    <rule ref="category/java/codestyle.xml/UselessQualifiedThis"/>

